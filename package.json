{
  "name": "@looksrare/sdk",
  "version": "0.12.1",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://test.gusdk.gu.net"
  },
  "keywords": [
    "looksrare"
  ],
  "homepage": "https://looksrare.org/",
  "bugs": "https://github.com/LooksRare/looksrare-sdk/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/LooksRare/looksrare-sdk.git"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "prepublishOnly": "yarn build",
    "build": "rollup -c && tsc -d",
    "dev": "rollup -c -w",
    "lint": "eslint --max-warnings 0 'src/**/*.{js,jsx,ts,tsx}'",
    "test:watch": "jest --watch",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,yaml,yml,md}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,yaml,yml,md}'",
    "release": "release-it --only-version --set-upstream",
    "build:sc": "hardhat compile",
    "test": "hardhat test"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,yaml,yml}": "yarn format:write"
  },
  "peerDependencies": {
    "ethers": "^5.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "ethers": "^5.5.4",
    "hardhat": "^2.13.0",
    "hardhat-abi-exporter": "^2.10.1",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "release-it": "^15.5.0",
    "rollup": "^2.69.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "solmate": "^6.7.0",
    "ts-jest": "^28.0.2",
    "typechain": "^8.1.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@0xsequence/multicall": "^0.43.25",
    "@looksrare/contracts-exchange-v1": "^1.4.1"
  }
}
